{{#with itemObject}}
	{{#if organizationtypes-strings}}
		<p class="post-categories {{../class}}">
			<em>{{../titles.types}}</em><br/>
			{{labelize organizationtypes-strings ../classes.label}}
		</p>
	{{/if}}
	{{#if organizationcategories-strings}}
		<p class="post-categories {{../class}}">
			<em>{{../titles.categories}}</em><br/>
			{{labelize organizationcategories-strings ../classes.label2}}
		</p>
	{{/if}}
	{{#if contact-person}}
		<p class="user-contactperson {{../class}}">
			<em>{{../titles.contactperson}}</em><br/>
			{{{contact-person}}}
		</p>
	{{/if}}
	{{#if contact-number}}
		<p class="user-contactnumber {{../class}}">
			<em>{{../titles.number}}</em><br/>
			{{{contact-number}}}
		</p>
	{{/if}}
{{/with}}